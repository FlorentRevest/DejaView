package {
    // See: http://go/android-license-faq
    // A large-scale-change added 'default_applicable_licenses' to import
    // all of the 'license_kinds' from "external_dejaview_license"
    // to get the below license kinds:
    //   SPDX-license-identifier-Apache-2.0
    default_applicable_licenses: ["external_dejaview_license"],
}

cc_test {
  name: "CtsDejaViewTestCases",
  team: "trendy_team_dejaview",
  srcs: [
    "device_feature_test_cts.cc",
    "heapprofd_java_test_cts.cc",
    "heapprofd_test_cts.cc",
    "producer_to_consumer_integrationtest_cts.cc",
    "traced_perf_test_cts.cc",
    ":dejaview_end_to_end_integrationtests",
    ":dejaview_protos_dejaview_config_cpp_gen",
    ":dejaview_protos_dejaview_common_cpp_gen"
  ],
  generated_headers: [
    "dejaview_protos_dejaview_config_cpp_gen_headers",
    "dejaview_protos_dejaview_common_cpp_gen_headers",
  ],
  static_libs: [
    "libgmock",
    "libprotobuf-cpp-lite",
    "libdejaview_client_experimental",
    "dejaview_cts_deps",
    "dejaview_trace_protos",
  ],
  whole_static_libs: [
    "dejaview_gtest_logcat_printer",
  ],
  shared_libs: [
    "libandroid",
    "liblog",
  ],
  test_suites: [
    "cts",
    "mts-art",
    "vts10",
    "general-tests",
    "mcts-art",
  ],
  compile_multilib: "both",
  multilib: {
    lib32: {
        suffix: "32",
    },
    lib64: {
        suffix: "64",
    },
  },
  per_testcase_directory: true,
  data: [
        ":CtsDejaViewNonProfileableApp",
        ":CtsDejaViewProfileableApp",
        ":CtsDejaViewProducerApp",
        ":CtsDejaViewReleaseApp",
        ":CtsDejaViewDebuggableApp",
        "dejaview_device_preparer.sh",
  ],
  stl: "libc++_static",
  // This test is also run via Mainline Testing against the ART Mainline
  // Module, which is updatable since Android 12 (API level 31).
  min_sdk_version: "31",
  defaults: [
    "dejaview_defaults",
  ],
}
